@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>
    <link rel="shortcut icon" type="image/png" href="~/Theming/images/logo.png" />
    
    <!-- Bootstrap Theme Style References -->
    <link href="~/Theming/styles/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <!-- Open Sans Style References -->
    <link href="https://fonts.googleapis.com/css?family=Open+Sans" rel="stylesheet">
    <!-- FontAwesome Style References -->
    <link href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css" rel="stylesheet" type="text/css">
    <!-- Custom Style References -->
    <link href="~/Theming/styles/Site.css" rel="stylesheet" type="text/css" />
</head>
<body>
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year</p>
        </footer>
    </div>
    <!-- JQuery References -->
    <script src="~/Theming/scripts/jquery-1.10.2.min.js"></script>
    <script src="~/Theming/scripts/jquery.unobtrusive-ajax.min.js"></script>

    <!-- Bootstrap References -->
    <script src="~/Theming/scripts/bootstrap.min.js"></script>
</body>
</html>
